    "url": "https://github.com/LBDserver/API/issues"
  },
  "homepage": "https://github.com/LBDserver/API#readme",
  "devDependencies": {
    "@types/express": "^4.17.10",
    "@types/node": "^14.14.20",
    "jsdoc-to-markdown": "^6.0.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "sparqlalgebrajs": "^2.4.0"
  }
